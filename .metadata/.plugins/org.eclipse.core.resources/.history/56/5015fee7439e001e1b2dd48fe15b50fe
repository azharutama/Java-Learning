package TUGAS_BESAR;

import java.io.Serializable;
import java.util.Scanner;

public class Nasabah implements Serializable {
    private int norek;
    private String Nama;
    private double saldo;
    private String pin;
    
    void BacaNilai() {
		Scanner sc=new Scanner (System.in);
		System.out.println("norek :");
		 norek =sc.nextInt();
		 System.out.println("Nama :");
		 Nama =sc.next();
		 System.out.println("UTS:");
		
		}
	
	void TulisNilai() {
		
		System.out.println("norek :" + norek);
		 
		 System.out.println("Nama :" + Nama);
		
		
		
		}

    public Nasabah(int norek, String Nama, double saldo, String pin) {
        this.norek = norek;
        this.Nama = Nama;
        this.saldo = saldo;
    }

    public Nasabah() {
		// TODO Auto-generated constructor stub
	}

	public int getnorek() {
        return norek;
    }
    public int setnorek() {
        return norek;
    }

    public String getNama() {
        return Nama;
    }
    public String setNama() {
        return Nama;
    }

    public double getsaldo() {
        return saldo;
    }
    public double setsaldo() {
        return saldo;
    }
    public String getpin() {
        return pin;
    }
    
    public String setpin() {
        return pin;
    }

    // Metode dan atribut lainnya
}
